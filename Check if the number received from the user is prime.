#include <iostream>
using namespace std;

int main()  {
    int n;
    cout<<"Bir eded daxil et:";
    cin>>n;
    
    if (n<=1)   {
        cout<<n<<"Sade eded deyil."<<endl;
        return 0;
    
    }
    int i;
    for (i=2;i*i<n;i++)
        if (n%i==0) break;
    cout<<n<<((i*i>n)?"Sade ededdir.":"sade eded deyil")<<endl;
    return 0;
}
